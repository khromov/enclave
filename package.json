{
	"name": "sveltekit-local-ai",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "PUBLIC_DISABLE_OPFS=false vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"lint:fix": "prettier --write . && eslint . --fix",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run"
	},
	"devDependencies": {
		"@eslint/compat": "^1.3.2",
		"@eslint/js": "^9.32.0",
		"@huggingface/jinja": "^0.5.1",
		"@sveltejs/adapter-node": "^5.2.14",
		"@sveltejs/kit": "^2.27.3",
		"@sveltejs/vite-plugin-svelte": "^6.1.1",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"@transcribe/transcriber": "^3.0.0",
		"@wllama/wllama": "github:khromov/wllama#cache-reset-fix-2",
		"eslint": "^9.32.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.11.0",
		"globals": "^16.3.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"subsrt-ts": "^2.1.2",
		"svelte": "^5.38.0",
		"svelte-check": "^4.3.1",
		"svelte-persisted-store": "^0.12.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.39.0",
		"vite": "^7.1.1",
		"vite-plugin-devtools-json": "^0.2.0",
		"vitest": "^3.2.3"
	},
	"dependencies": {
		"@fontsource/bebas-neue": "^5.0.20",
		"@fontsource/space-grotesk": "^5.0.20",
		"express": "^5.1.0"
	}
}
